{
  "app": {
    "name": "den-den-mushi-proxy",
    "environment": "dev",
    "version": "1.0.0",
    "port": 45007
  },
  "ssl": {
    "enabled": false
  },
  "development": {
    "isSMX": false,
    "UseSqlJtiRepo": true,
    "UseRedis": false,
    "IsUsingInvDb": false,
    "IsAutoMigrateEnabled": true,
    "RegexDbPollIntervalSeconds": 10
  },
  "logger": {
    "level": "debug",
    "format": "console",
    "output": "both",
    "filePath": "./log/ddm-proxy.log"
  },
  "Cors": {
    "allowOrigins": ["*"],
    "allowMethods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"],
    "allowHeaders": ["Authorization", "Content-Type", "X-Requested-With"],
    "exposeHeaders": [],
    "allowCredentials": true,
    "allowMaxAge": 3600
  },
  "Puppet": {
    "InjectPublicKeyTaskName": "injectkey",
    "RemovePublicKeyTaskName": "removekey",
    "TaskRetrySeconds": 5,
    "RetryAttempts": 1,
    "TaskNode": "xxx01sssmmm"
  },
  "JwtAudience": {
      "ExpectedIssuer": "control-server",
      "ExpectedAudience": "OS",
      "ExpectedTyp": "proxy/ws+jwt"
  },
  "Websocket": {
    "PingPong": {
      "PingIntervalSeconds": 30,
      "PingTimeoutSeconds": 10,
      "MaxPingMissed": 10,
      "PongWaitSeconds": 10
    }
  },
  "DdmDB": {
    "user": "root",
    "port": 3306,
    "dbName": "DenDenMushi",
    "params": "charset=utf8mb4&parseTime=True&loc=Local",
    "maxIdleConns": 2,
    "maxOpenConns": 10,
    "connMaxLifetimeMinutes": 30,
    "sslEnabled": false,
    "logLevel": 4
  },
  "InvDB": {
    "user": "",
    "port": 4405,
    "dbName": "",
    "params": "charset=utf8mb4&parseTime=True&loc=Local",
    "maxIdleConns": 1,
    "maxOpenConns": 10,
    "connMaxLifetimeMinutes": 30,
    "sslEnabled": true,
    "logLevel": 4
  },
  "redis": {
    "port": 6379,
    "poolSize": 10
  },
  "Ssh": {
    "IsLocalSshKeyEnabled": true,
    "IsRemoveInjectKeyEnabled": true,
    "IsRSAKeyPair": false,
    "IsCleanupEnabled": true,
    "IsLogPrivateKey": true,
    "ConnectDelayAfterInjectSeconds": 5,
    "SshCommand": "ssh"
  },
  "Pty": {
    "SpawnRetryCount": 4,
    "SpawnRetryIntervalSeconds": 5
  },
  "PuppetTasks": {
    "QueryJobs": {
      "OrchestratorEndpoint": "https://orchestrator/v1/jobs",
      "WaitBeforeQuerySeconds": 5,
        "MaxQueryAttempts": 5,
        "QueryIntervalSeconds": 10
    },
    "CyberarkPasswordDraw": {
      "Environment": "software",
      "TaskName": "cyberark-password-draw",
      "CybidA": "abc",
      "CybidB": "123",
      "SafeA": "asd",
      "SafeB": "qwe",
      "IsValidationEnabled": true
    }
  },
  "Tmpauth": {
    "IsEnabled": false,
    "UserIdKey": "username",
    "OuGroupKey": "ou"
  }
}
